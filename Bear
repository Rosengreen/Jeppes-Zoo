public class Bear
{
    private String gender;
    private int legs = 4;
    private String sound;
    
    public Bear(String gender, int legs, String sound)
      {
        this.gender = gender;
        this.sound = sound;    
      }

    public String getGender()
      {
        return gender;
      }

    public String getSound()
      {
        return sound;
      }
}
