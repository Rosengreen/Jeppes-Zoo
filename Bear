public class Bear
{
    private String gender;
    private int legs;
    private String sound;
    
    public Bear(String gender, int legs, String sound)
      {
        this.gender = gender;
        this.legs = legs;
        this.sound = sound;    
      }

    public String getGender()
      {
        return gender;
      }
    public int getLegs()
      {
        return legs;
      }
    public String getSound()
      {
        return sound;
      }
}
